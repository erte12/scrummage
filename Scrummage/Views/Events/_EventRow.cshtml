<tr data-event-id="{{id}}">
    <td class="event-content-col">
        {{content}}
    </td>
    <td class="event-date-col">
        {{startsAt}}
    </td>
    <td class="event-date-col">
        {{endsAt}}
    </td>
    <td class="event-delete-col">
        <button class="btn btn-sm btn-default center-block js-delete">
            <span class="glyphicon glyphicon-trash"></span>
        </button>
    </td>
</tr>